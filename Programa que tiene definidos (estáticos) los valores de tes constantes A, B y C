#include <stdio.h>
int main (void){
	int A=5,B=3,C=-12;

	{
	if (A>3)
	printf("A(%d) es mayor que 3 \n",A);
	else
	printf("A(%d) NO es mayor que 3 \n",A);
	}
	{
	if (A>C)
	printf("A(%d) es mayor que C(%d) \n",A, C);
	else
	printf("A(%d) NO es mayor que C(%d) \n",A, C);
	}	
	{
	if (B!=C)
	printf("B(%d) es diferente a C(%d) \n",B,C);
	else
	printf("B(%d) NO es diferente a C(%d) \n",B,C);
	}	
	{
	if (A==3)
	printf("A(%d) es igual a 3 \n",A);
	else
	printf("A(%d) NO es igual a 3 \n",A);
	}	
	{
	if (A*B==15)
	printf("La multiplicacion de A(%d)*B(%d) es igual a 15 \n",A,B);
	else
	printf("La multiplicacion de A(%d)*B(%d) NO es igual a 15 \n",A,B);
	}
	{	
	if (C/B<A)
	printf("La division de C(%d)/B(%d) es menor que A(%d) \n",C,B,A);
	else
	printf("La division de C(%d)/B(%d)  NO es menor que A(%d) \n",C,B,A);
	}	
	{	
	if (C/B==-10)
	printf("La division de C(%d)/B(%d) es igual a -10 \n",C,B);
	else
	printf("La division de C(%d)/B(%d)  NO es igual -10 \n",C,B);
	}
	{	
	if (A+B+C==5)
	printf("La suma de A(%d)+B(%d)+C(%d) es igual a 5 \n",A,B,C);
	else
	printf("La suma de A(%d)+B(%d)+C(%d) NO es igual a 5 \n",A,B,C);
	}
	{
	if ((A+B==8)&&(A-B==2))
	printf("La suma de A(%d)+B(%d) es igual a 8 y su resta es igual a 2 \n",A,B);
	else
	printf("La suma de A(%d)+B(%d) NO es igual a 8 y su resta NO es igual a 2 \n",A,B);
	}
	{
	if ((A+B==8) && (A-B==6))
	printf("La suma de A(%d)+B(%d) es igual a 8 y su resta es igual a 6 \n",A,B);
	else
	printf("La suma de A(%d)+B(%d) NO es igual a 8 y su resta NO es igual a 6 \n",A,B);
	}
	{
	if (A>3 && B>3 && C<3)
	printf("A(%d) y B(%d) son mayores que 3 y C(%d) es menor que 3 \n",A,B,C);
	else
	printf("A(%d) y B(%d) NO son mayores que 3 y C(%d) NO es menor que 3 \n",A,B,C);
	}
	return 0;
	}
