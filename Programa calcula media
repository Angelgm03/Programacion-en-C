#include <stdio.h>
#include <math.h>

int main(void) {
	int arr[30]={20,19,18,16,19,20,22,21,19,20,22,24,21,26,22,20,19,18,16,19,20,22,21,19,20,22,24,21,26,19};
	int i,sum;
	float prom;
	sum=0;
	for(i=0;i<30;i++) {
	sum=sum+arr[i];	}
	prom=sum/30;
	printf("la media de las temperaturas del mes es %1.2f \n",prom);
	return 0;
}
