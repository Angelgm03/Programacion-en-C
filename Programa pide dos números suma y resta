 #include <stdio.h>
  
  int main (void) {
  int a, b, sum, dif, mul;
  printf ("Indica el valor del primer numero: \n");
  scanf ("%d", &a);
  printf ("Indica el valor del segundo numero: \n");
  scanf ("%d", &b);
  sum=a+b;
  printf(" La suma de los dos numeros es %d \n",sum);
  dif= a-b;
  printf(" La resta de los dos numeros es %d \n",dif);
  mul= a*b;
  printf(" La multiplicacion de los dos numeros es %d \n",mul);
return 0;

}
